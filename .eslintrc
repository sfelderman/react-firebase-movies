{
    "plugins": [
        "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        },
        "env": { "es6": true },
        "node": true
    },
    "rules": {
        "react/jsx-uses-vars": [2],
        "react/jsx-uses-react": [2],
        "quote-props": [1, "consistent-as-needed"],
        "no-cond-assign": [2, "except-parens"],
        "no-console": 0,
        "radix": 0,
        "space-infix-ops": 0,
        "no-unused-vars": [1, {"vars": "local", "args": "none"}],
        "default-case": 0,
        "no-else-return": 0,
        "no-param-reassign": 0,
        "no-extra-semi": 1,
        "semi": 1,
        "no-undef": 1
      }
}
